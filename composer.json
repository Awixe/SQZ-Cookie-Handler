{
    "name": "zeroastro/sqz-cookie-handler",
    "description": "This is a Simple PHP Cookie Handler which uses the OpenSSL extension to perform data encryption/decryption",
    "license": "MIT",
    "keywords": ["cookie", "cookie handler"],
    "homepage": "https://github.com/zeroastro/SQZ-Cookie-Handler",
    "authors": [{
        "name": "Salvatore Q Zeroastro",
        "email": "zeroastro@gmail.com",
        "role": "Creator"
    }],
    "minimum-stability" : "dev",
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "SQZ_CookieHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SQZ_CookieHandler_Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
